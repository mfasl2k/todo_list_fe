{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://todo-list-be-three.vercel.app/api/:path*"
    },

    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "vite"
}
